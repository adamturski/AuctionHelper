<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="view" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="400.0" prefWidth="600.0" style="-fx-padding: 10px;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.com.turski.ah.view.checkResults.CheckResultsController">
    <children>
        <HBox alignment="CENTER_LEFT" prefHeight="47.0" prefWidth="580.0" style="-fx-spacing: 10px;" VBox.vgrow="NEVER">
            <children>
                <Button fx:id="copyToClipboardButton" mnemonicParsing="false" onAction="#copyToClipboardAction" text="Kopiuj do schowka" />
            <Button fx:id="showInBrowserButton" mnemonicParsing="false" onAction="#showInBrowserAction" text="Wyświetl w przeglądarce" />
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <TextArea fx:id="filledTemplateText" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
         </children>
        </HBox>
    </children>
</VBox>
